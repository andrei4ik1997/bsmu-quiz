{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"bsmu-quiz": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"changeDetection": "OnPush"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "src/main.ts",
						"tsConfig": "tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							},
							{
								"glob": "**/*",
								"input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
								"output": "/assets/"
							},
							"src/service-worker.js"
						],
						"styles": ["src/styles.scss"],
						"stylePreprocessorOptions": {
							"includePaths": ["node_modules", "src/app/styles"]
						},
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "4MB",
									"maximumError": "5MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "10kB",
									"maximumError": "16kB"
								}
							],
							"outputHashing": "all",
							"optimization": {
								"scripts": true,
								"styles": {
									"minify": true,
									"inlineCritical": true
								},
								"fonts": false
							},
							"serviceWorker": "ngsw-config.json"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "bsmu-quiz:build:production"
						},
						"development": {
							"buildTarget": "bsmu-quiz:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
					}
				},
				"extract-i18n": {
					"builder": "@angular/build:extract-i18n"
				},
				"deploy": {
					"builder": "angular-cli-ghpages:deploy",
					"options": {
						"buildTarget": "bsmu-quiz:build:production",
						"baseHref": "https://andrei4ik1997.github.io/bsmu-quiz/",
						"dir": "dist/bsmu-quiz/browser",
						"name": "Andrei Shinkarev",
						"email": "andrey03071997@yandex.ru",
						"message": "Deploy"
					}
				}
			}
		}
	},
	"schematics": {
		"@schematics/angular:component": {
			"type": "component"
		},
		"@schematics/angular:directive": {
			"type": "directive"
		},
		"@schematics/angular:service": {
			"type": "service"
		},
		"@schematics/angular:guard": {
			"typeSeparator": "."
		},
		"@schematics/angular:interceptor": {
			"typeSeparator": "."
		},
		"@schematics/angular:module": {
			"typeSeparator": "."
		},
		"@schematics/angular:pipe": {
			"typeSeparator": "."
		},
		"@schematics/angular:resolver": {
			"typeSeparator": "."
		}
	}
}
